<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#CCCCCC"
    android:orientation="vertical"

    android:layout_height="match_parent"
    tools:context="co.com.newpandatv.view.activity.SQLActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:background="@color/colorPrimaryDark"
        android:layout_height="45dp">
        <ImageView
            android:layout_marginLeft="5dp"
            android:id="@+id/sql_IMG"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:src="@mipmap/personal_back_img"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="25sp"
            android:layout_weight="1"
            android:text="收藏"
            android:textColor="@color/biz_audio_progress_first"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            />
   </LinearLayout>
    <co.com.newpandatv.view.listview.MyListView
        android:layout_marginTop="3dp"
        android:layout_width="match_parent"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:id="@+id/sqlListView"
        android:divider="#00000000"
        android:dividerHeight="3dp"
        android:layout_height="match_parent">
        </co.com.newpandatv.view.listview.MyListView>
</LinearLayout>
