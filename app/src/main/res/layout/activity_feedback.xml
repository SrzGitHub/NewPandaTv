<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"

    android:layout_height="match_parent"
    tools:context="co.com.newpandatv.view.activity.setup.FeedbackActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:background="@color/colorPrimaryDark"
        android:layout_height="45dp">
        <ImageView
            android:layout_marginLeft="5dp"
            android:id="@+id/one_IMG"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:src="@mipmap/personal_back_img"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:layout_marginRight="30dp"
            android:layout_weight="1"
            android:text="用户反馈"
            android:textColor="@color/biz_audio_progress_first"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            />

    </LinearLayout>

    <android.support.design.widget.TabLayout
        android:id="@+id/fee_Tyl"
        android:layout_width="fill_parent"
        app:tabTextAppearance="@style/TabText"
        app:tabMode="scrollable"
        app:tabSelectedTextColor="@color/colorPrimaryDark"
        app:tabIndicatorColor="@color/colorPrimaryDark"
        android:background="@color/color_text_selected"
        android:layout_height="40dp">



    </android.support.design.widget.TabLayout>
    <android.support.v4.view.ViewPager
        android:id="@+id/fee_Vip"

        android:layout_width="match_parent"
        android:layout_height="match_parent">


    </android.support.v4.view.ViewPager>

</LinearLayout>
